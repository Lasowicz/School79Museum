<+>@font-face {\r\n    font-family: Nunito, Arial, sans-serif, serif;\r\n    src: url(../Fonts/Nunito-Regular.woff2);\r\n    font-weight: normal;\r\n    font-style: normal;\r\n\r\n}\r\n/* ОФОРМЛЕНИЕ МЕНЮ И ЕГО ЭЛЕМЕНТОВ*/\r\nheader {\r\n    display: flex;\r\n    flex-flow: row nowrap;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n}\r\n\r\n.main-menu {\r\n    background-color: darkred;\r\n    height: 60px;\r\n    font-family: Nunito, Arial, sans-serif, serif;\r\n    font-weight: bold;\r\n    display: flex;\r\n    flex-flow: row nowrap;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.contacts {\r\n    margin: 20px;\r\n}\r\n.contacts a {\r\n    text-decoration: none;\r\n    padding: 2px;\r\n    color: white;\r\n}\r\n.menu {\r\n    flex-grow: 1;\r\n}\r\n\r\n.menu ul {\r\n    text-decoration: none;\r\n    display: flex;\r\n    flex-flow: row nowrap;\r\n    margin: 0;\r\n    padding: 0;\r\n    list-style: none;\r\n}\r\n.menu li {\r\n    margin-right: 20px;\r\n    text-decoration: none;\r\n}\r\n\r\n.menu a {\r\n    color: white;\r\n    text-decoration: none;\r\n    padding: 8px 10px;\r\n}\r\n\r\n#logo {\r\n    background-image: url(../Images/schoolLogo.png);\r\n    margin: 20px;\r\n    width: 343px;\r\n    height: 61px;\r\n}\r\n#collage {\r\n    border: 1px solid black;\r\n    background-image: url(../Images/header_collage.png);\r\n    height: 61px;\r\n    width: 1150px;\r\n    margin-right: 10px;\r\n}\r\n\r\n.contacts:hover, .menu li:hover, #logo:hover,  #collage:hover {\r\n    transform: scale(102%);\r\n    transition: 1s;\r\n}\r\n\r\n                                         /*MAIN CONTENT*/\r\n.main-content {\r\n    background-image: url(../Images/Shaimuratov.jpeg);\r\n    height: 924px;\r\n    background-repeat: no-repeat;\r\n    width: 921px;\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 0 20px;\r\n}\r\n#memorable_and_history_date{\r\n    font-family: \"Times New Roman\", Arial, sans-serif, serif;\r\n    color: white;\r\n    font-size: 19px;\r\n    background-color: darkred;\r\n    text-align: center;\r\n    height: 50px;\r\n    width: 350px;\r\n    cursor: pointer;\r\n    position: fixed;\r\n    right: 100px;\r\n    top: 50%;\r\n\r\n}\r\n#memorable_and_history_date:hover {\r\n    transform: scale(102%);\r\n    transition: 1s;\r\n}\r\n\r\n                                        /*BASEMENT*/\r\n\r\n.basement {\r\n    background-color: darkred;\r\n    height: 61px;\r\n}
